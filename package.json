{
  "name": "crescendoclub",
  "private": true,
  "version": "0.0.0",
  "prettier": {
    "semi": false,
    "singleQuote": true,
    "tabWidth": 2,
    "useTabs": false,
    "trailingComma": "all",
    "printWidth": 80
  },
  "type": "module",
  "scripts": {
    "dev": "vite",
